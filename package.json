{
  "name": "agentguard-sdk",
  "version": "0.1.1",
  "description": "TypeScript/JavaScript SDK for AI Agent Security Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ai",
    "security",
    "agent",
    "sdk",
    "typescript",
    "javascript",
    "langchain",
    "openai",
    "anthropic",
    "claude",
    "policy",
    "governance",
    "compliance",
    "audit",
    "guardrails",
    "llm",
    "chatbot",
    "automation",
    "middleware",
    "protection"
  ],
  "author": "AI Agent Security Platform",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentguard-ai/agentguard-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/agentguard-ai/agentguard-sdk/issues"
  },
  "homepage": "https://github.com/agentguard-ai/agentguard-sdk#readme",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
